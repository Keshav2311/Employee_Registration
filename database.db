-- Table: public.registration

-- DROP TABLE IF EXISTS public.registration;

CREATE TABLE IF NOT EXISTS public.registration
(
    id integer NOT NULL,
    first_name character varying(20) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    last_name character varying(20) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    username character varying(250) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    password character varying(20) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    address character varying(45) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    contact character varying(45) COLLATE pg_catalog."default" DEFAULT NULL::character varying,
    CONSTRAINT registration_pkey PRIMARY KEY (id)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.registration
    OWNER to postgres;
